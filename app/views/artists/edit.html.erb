<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Edit An Artist</title>
  </head>
  <body>
    <%= form_for @artist do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name %>
      <%= f.label :bio %>
      <%= f.text_field :bio %>
      <%= f.submit %>
    <% end %>
  </body>
</html>
